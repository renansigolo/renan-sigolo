    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.1",
    "next": "^11.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "concurrently": "^6.2.0",
    "cypress": "^7.5.0",
    "eslint": "^7.28.0",
    "eslint-config-next": "^11.0.0",
    "postcss": "^8.3.4",
    "prettier": "^2.3.1",
    "tailwindcss": "^2.1.4"
